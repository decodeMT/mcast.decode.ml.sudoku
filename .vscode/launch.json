    "version": "0.2.0",
    "configurations": [
        {
            "name": "Solve single",
            "type": "python",
            "request": "launch",
            "program": "sudokuSolver.py",
            "python": "${fileDirname}/sudoku-venv/Scripts/python.exe",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["./output/puzzles.txt", "./output/solutions.txt","./output/stats.csv", "./output/tracking.txt", "./output/errors.txt", "0", "1", "1", "1"]
        },
        {
            "name": "Solve multiple",
            "type": "python",
            "request": "launch",
            "program": "sudokuSolver.py",
            "python": "${fileDirname}/sudoku-venv/Scripts/python.exe",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["./output/puzzles.txt", "./output/solutions.txt","./output/stats.csv", "./output/tracking.txt", "./output/errors.txt", "0", "100", "1", "1"]
        },
        {
            "name": "Generate sudoku puzzles",
            "type": "python",
            "request": "launch",
            "program": "sudokuGenerator.py",
            "python": "${fileDirname}/sudoku-venv/Scripts/python.exe",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["50", "26", "30", "./output/puzzles.txt"]
        }
        ,
        {
            "name": "Evaluate dataset",
            "type": "python",
            "request": "launch",
            "program": "datasetEval.py",
            "python": "${fileDirname}/sudoku-venv/Scripts/python.exe",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["./output/puzzles.txt", "./output/datasetZeros.PNG", "./output/datasetDifficulty.PNG"]
        }
        ,
        {
            "name": "Evaluate prototype",
            "type": "python",
            "request": "launch",
            "program": "prototypeEval.py",
            "python": "${fileDirname}/sudoku-venv/Scripts/python.exe",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["./output/puzzles.txt", "./output", "0", "100"]
        }
    ]
}